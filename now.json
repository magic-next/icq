{
  "version": 2,
  "alias": "magic-icq",
  "builds": [
    {
      "src": "/src/app.js",
      "use": "@now/node",
      "config": {
        "includeFiles": ["build/**", "emails/**"]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/src/app.js" }
  ],
  "build": {
    "env": {}
  },
  "env": {
    "APPLICATION_KEY": "@icq-key",
    "SMTP_USER": "@smtp-user",
    "SMTP_HOST": "@smtp-host",
    "SMTP_PASSWORD": "@smtp-pass",
    "EMAIL_SENDER": "Equipe Magic Next <suporte@magicnext.com.br>"
  }
}